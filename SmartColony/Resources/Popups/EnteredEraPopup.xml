<?xml version="1.0"?>
<context>
    <anchorx>center</anchorx>
    <anchory>center</anchory>
    <offsetx>0</offsetx>
    <offsety>0%</offsety>
    <width>90%</width>
    <height>90%</height>
    <background>grid9_dialog</background>
    
    <!-- sub items -->
    <items>
        <item>
            <identifier>popup_image</identifier>
            <type>image</type>
            <anchorx>center</anchorx>
            <anchory>center</anchory>
            <offsetx>0</offsetx>
            <offsety>110</offsety>
            <width>100</width>
            <height>100</height>
            <image>questionmark</image>
        </item>
        <item>
            <identifier>popup_title</identifier>
            <type>label</type>
            <title>Entered Era</title>
            <anchorx>center</anchorx>
            <anchory>center</anchory>
            <offsetx>0</offsetx>
            <offsety>50</offsety>
            <width>300</width>
            <height>40</height>
        </item>
        <item>
            <identifier>popup_summary</identifier>
            <type>label</type>
            <title>You have entered era</title>
            <fontSize>24</fontSize>
            <anchorx>center</anchorx>
            <anchory>center</anchory>
            <offsetx>0</offsetx>
            <offsety>20</offsety>
            <width>300</width>
            <height>40</height>
        </item>
        
        <item>
            <identifier>okay_button</identifier>
            <type>button</type>
            <title>Okay</title>
            <result>OKAY</result>
            <anchorx>center</anchorx>
            <anchory>center</anchory>
            <offsetx>0</offsetx>
            <offsety>-150</offsety>
            <width>200</width>
            <height>42</height>
        </item>
    </items>
</context>
